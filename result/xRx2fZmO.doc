********************/

body {
  font-family: 'Open Sans', sans-serif;
}

#wrapper {
  max-width: 940px;
  margin: 0 auto;
  padding: 0 5%;
}

img {
  max-width: 100%;
}

h3 {
  margin: 0 0 1em 0;
}
/********************
HEADER
********************/

header {
  float: left;
  margin: 0 0 30px 0;
  padding: 5px 0 0 0;
  width: 100%;
}


/* logo text */
#logo {
  text-align: center;
  margin: 0;
}

h1 {
  font-family: 'Changa One', sans-serif;
  margin: 50px 0;
  font-size: 1.75em;
  font-weight: normal;
  line-height: 0.8em;
}

h2 {
  font-size: 0.75em;
  margin: -45px 0 0;
  font-weight: normal;
}



/********************
NAVIGATION
********************/


nav {
  text-align: center;
  padding: 10px 0;
  margin: 20px 0 0;
}

nav ul {
  list-style: none;
  margin: 0 10px;
  padding: 0;
}

nav li {
  display: inline;
}

nav a {
  font-weight: 800;
  padding: 15px 10px;
}

/********************
FOOTER
********************/



footer {
  font-size: 0.75em;
  text-align: center;
  clear: both;
  padding-top: 50px;
  color: #ccc;
}

.social-icon {
  width: 20px;
  height: 20px;
  margin: 0 5px;
}

/********************
PAGE PORTFOLIO
********************/



#gallery {
  margin: 0;
  padding: 0;
  list-style: none;
}

#gallery li {
  float: left;
  width: 45%;
  margin: 2.5%;
  background-color: #f5f5f5;
}

#gallery li a p {
  margin: 0;
  padding: 5%;
  font-size: 0.75em;
}


/********************
PAGE: ABOUT
********************/

.portfolio_photo {
  display: block;
  max-width: 170px;
  margin: 0 auto 30px;
  border-radius: 100%;
}


/********************
PAGE: CONTACT
********************/


.contact-info {
  list-style: none;
  padding: 0;
  margin: 0;
  font-size: 0.9em;
}

.contact-info a {
  display: block;
  min-height: 20px;
  background-repeat: no-repeat;
  background-size: 20px 20px;
  padding: 0 0 0 30px;
  margin: 0 0 10px;
  color: #999;
}

.contact-info li.phone-life a {
  background-image: url('../img/life.png');
}

.contact-info li.phone-mts a {
  background-image: url('../img/mts.gif');
}

.contact-info li.mail a {
  background-image: url('../img/gmail.gif');
}

/********************
COLORS
********************/



/* site body */
body {
  background-color: #fff;
  color: #999;
}

/* light blue header #7E7EFF */
/* border color blue #196CE8 */
header {
  background: #7E7EFF;
  border-color: #196CE8;
}

/* white headings #fff */
h1, h2 {
  color: #fff;
}

/* middle blue #196CE8 */
nav {
  background: #196CE8;
}

/* nav link color white */
nav a, nav a:visited {
  color: #fff;
}

/* nav link selected color and hover color */ 
nav a.selected, nav a:hover {
  color: #8AB8E6;
}

/* links decoration and color */
a {
  text-decoration: none;
  color: #525252;
}

/* tic-tac-toe styles */

#Games {
  clear: both;
  margin: 20px 0 0 0;
  padding: 0;
  list-style: none;
}

#Games li {
  float: left;
  width: 28%;
  margin: 2.5%;
  background-color: #f5f5f5;
}

#Games li a p {
  margin: 0;
  padding: 5%;
  font-size: 0.75em;
}

#field {
  clear: both;
  display: block;
  width: 53%; /* 500 / 940 = 0.53 = 53% */
  margin: auto;
}

#field table {
  width: 100%; /* 500 / 940 = 0.53 = 53% */
  height: 100%;
  background-color: #000;
}

#field table tr td {
  width: 26%; /* 128 / 500  = 26%*/
  height: 26%;
  border: 2px solid black;
  background-color: #FFFFCC;
}

#clearField {
  margin: 20px auto auto 28%;
  border-radius: 0;
}

/*styling background images*/
.cross {
   background-image: url("../Games/image/Cross.png");
   background-repeat: no-repeat;
   background-position: center;
   background-size: 99%;
}

.zero {
   background-image: url("../Games/image/zero.png");
   background-repeat: no-repeat;
   background-position: center;
   background-size: 99%;
}</style>